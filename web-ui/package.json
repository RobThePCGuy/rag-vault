{
  "name": "rag-vault-ui",
  "private": true,
  "version": "1.5.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "type-check": "tsc -b",
    "lint": "biome lint src",
    "lint:fix": "biome lint --write src",
    "format": "biome format --write src",
    "format:check": "biome format src",
    "check": "pnpm type-check && pnpm lint && pnpm format:check",
    "check:deps": "madge --circular --extensions ts,tsx src",
    "check:unused": "node scripts/check-unused-exports.cjs",
    "check:all": "pnpm check && pnpm check:unused && pnpm check:deps",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.62.16",
    "@tanstack/react-virtual": "^3.13.18",
    "framer-motion": "^12.34.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.5",
    "react-router-dom": "^7.13.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.12",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "jsdom": "^27.4.0",
    "madge": "^8.0.0",
    "typescript": "^5.7.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
