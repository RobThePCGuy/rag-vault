{
  "name": "rag-vault-ui",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "type-check": "tsc -b",
    "lint": "biome lint src",
    "lint:fix": "biome lint --write src",
    "format": "biome format --write src",
    "format:check": "biome format src",
    "check": "pnpm type-check && pnpm lint && pnpm format:check",
    "check:deps": "madge --circular --extensions ts,tsx src",
    "check:unused": "node scripts/check-unused-exports.cjs",
    "check:all": "pnpm check && pnpm check:unused && pnpm check:deps"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.62.16",
    "framer-motion": "^11.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.5",
    "react-router-dom": "^7.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.12",
    "madge": "^8.0.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.2",
    "vite": "^6.0.5"
  }
}
